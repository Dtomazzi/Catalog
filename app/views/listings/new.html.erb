<h2> Add a new Listing </h2>


 <%= form_for @listing do |f| %>

<p>
 <%= f.label :title %>
 <%= f.text_field :title %>
</p>

<p>
 <%= f.label :price %>
 <%= f.number_field :price %>
</p>

<p>
  <%= f.label :description %>
  <%=f.text_area :description %>
</p>

<p>
  <%=f.label :contact %>
  <%=f.text_field :contact %>
</p>


 <%= f.submit "Create new listing!" %>

 <% end %>
